package com.small.library.taglib;

import java.io.Serializable;

/****************************************************************************
*
*	Interface that represents an item in a list of options.
*
* 	@author David Small
* 	@version 1.0.0.0
* 	@date 12/30/2002
* 
*	@see SelectTag
*	@see SelectionTag
*	@see OptionButtonsElement
*	@see CheckBoxesTag
*	@see RadioButtonsTag
*
*****************************************************************************/

public interface ListItem extends Serializable
{
	/** Accessor method - gets the ID that represents the list item. */
	public String getListItemId();

	/** Accessor method - gets the description that represents the list
	    item.
	*/
	public String getListItemDesc();
}

